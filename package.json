{
  "name": "troxel",
  "version": "0.9.0",
  "description": "WebGL-based voxel viewer and editor",
  "homepage": "https://chrmoritz.github.io/Troxel/",
  "bugs": {
    "url": "https://github.com/chrmoritz/Troxel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:chrmoritz/Troxel.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "./tools/index.js",
  "scripts": {
    "start": "coffee tools/devServer.coffee",
    "test": "mocha -t 5000 --compilers coffee:coffee-script/register",
    "watch": "./node_modules/.bin/jade views/index.jade --out . --watch",
    "prepublish": "jspm install"
  },
  "author": "Christian Moritz",
  "license": "LGPL-3.0",
  "devDependencies": {
    "coffeelint": "^1.14.1",
    "jade": "^1.11.0",
    "jspm": "^0.17.0-beta.5",
    "mocha": "^2.3.0",
    "should": "^8.0.0"
  },
  "dependencies": {
    "coffee-script": "^1.10.0"
  },
  "jspm": {
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "coffee": "github:forresto/system-coffee@^0.1.2",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "jquery": "github:components/jquery@^2.2.0",
      "stats": "github:mrdoob/stats.js@r14",
      "three": "github:mrdoob/three.js@r71",
      "typeahead": "github:twitter/typeahead.js@0.10.5"
    },
    "devDependencies": {
      "plugin-typescript": "github:frankwallis/plugin-typescript@^2.5.9"
    },
    "overrides": {
      "github:mrdoob/three.js@r71": {
        "main": "build/three",
        "ignore": [
          "docs",
          "src",
          "editor",
          "test",
          "utils",
          "examples/*.html",
          "examples/models",
          "examples/scenes",
          "examples/sounds",
          "examples/obj",
          "examples/textures"
        ],
        "format": "global",
        "meta": {
          "build/three.js": {
            "exports": "THREE"
          },
          "examples/js/shaders/SSAOShader.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/shaders/FXAAShader.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/shaders/CopyShader.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/postprocessing/ShaderPass.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/postprocessing/RenderPass.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/postprocessing/MaskPass.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          },
          "examples/js/postprocessing/EffectComposer.js": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          }
        }
      },
      "github:twitter/typeahead.js@0.10.5": {
        "dependencies": {
          "jquery": "github:components/jquery"
        }
      },
      "github:components/jquery@2.2.0": {
        "main": "jquery"
      },
      "github:twbs/bootstrap@3.3.6": {
        "shim": {
          "js/bootstrap": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css",
          "less",
          "grunt",
          "LICENSE"
        ],
        "systemjs": {
          "main": "dist/js/bootstrap.js",
          "modules": {
            "dist/js/bootstrap.js": {
              "deps": [
                "jquery"
              ],
              "exports": "$"
            }
          }
        }
      },
      "npm:typescript@1.7.5": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "os": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
